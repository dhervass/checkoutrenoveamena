{
  "name": " @amena/cesta-standalone",
  "version": "1.0.0-SNAPSHOT.0",
  "keywords": [
    "Angular 5",
    "Angular",
    "Orange Module"
  ],
  "author": "Everis Spain",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.js --app demo --port 3001",
    "mockserver": "ng serve --proxy-config proxy-mock.conf.js --app demo",
    "postinstall": "npm rebuild node-sass",
    "build": "ng-packagr -p package.json",
    "test": "ng test --browsers PhantomJS",
    "test:local": "ng test --browsers Chrome --singleRun false",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "compodoc": "compodoc -p tsconfig-compodoc.json -d docs --theme material --hideGenerator --coverageTestThresholdFail false --disableSourceCode --disableDomTree --disableGraph --disablePrivate --disableProtected --disableInternal"
  },
  "ngPackage": {
    "lib": {
      "$schema": "./node_modules/ng-packagr/ng-package.schema.json",
      "cssUrl": "inline",
      "comments": "none",
      "licensePath": "LICENSE",
      "umdModuleIds": {
        "lodash": "_",
        "moment": "moment",
        "@nsalaun/ng-logger": "ngLogger",
        "@ngrx/store": "ngStore",
        "@ngrx/effects": "ngEffects",
        "@orange/core": "ngCore",
        "@orange/redux": "ngRedux",
        "@ng-bootstrap/ng-bootstrap": "Ngb",
        "ng-boosted": "NgBoosted"
      },
      "entryFile": "src/public_api.ts"
    },
    "whitelistedNonPeerDependencies": [
      "."
    ]
  },
  "dependencies": {},
  "devDependencies": {
    "@orange/core": "1.1.21-SNAPSHOT.0",
    "@orange/redux": "1.0.350-SNAPSHOT.0",
    "@orange/boostedosp": "0.0.2-SNAPSHOT",
    "@compodoc/compodoc": "1.1.3",
    "jquery": "^3.3.1",
    "popper.js": "^1.15.0",
    "@orange/utilitiesosp": "0.0.1-SNAPSHOT",
    "@types/jasmine": "2.8.8",
    "@types/jasminewd2": "2.0.3",
    "@types/mocha": "5.2.5",
    "@types/node": "6.0.114",
    "chai": "4.2.0",
    "codelyzer": "4.3.0",
    "jasmine": "3.1.0",
    "jasmine-core": "3.1.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "2.0.3",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage": "1.1.2",
    "karma-coverage-istanbul-reporter": "2.0.1",
    "karma-jasmine": "1.1.2",
    "karma-jasmine-html-reporter": "1.1.0",
    "karma-mocha": "1.3.0",
    "karma-mocha-reporter": "2.2.5",
    "karma-phantomjs-launcher": "1.0.4",
    "karma-remap-istanbul": "0.6.0",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "3.0.0",
    "mocha": "5.2.0",
    "ng-packagr": "2.4.5",
    "protractor": "5.3.2",
    "sinon-chai": "3.2.0",
    "ts-node": "7.0.0",
    "tslint": "5.10.0",
    "typescript": "2.6.2"
  }
}
